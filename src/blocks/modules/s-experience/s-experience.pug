include ../../components/sect-header/sect-header
include ../../components/sect-subheader/sect-subheader
include ../../components/experience-item/experience-item

- var exp_imgPath_webp = 'img/general/folder_desktop.webp';
- var exp_imgPath_png = 'img/general/folder_desktop.png';
- var imgPlaceholder = 'data:image/gif;base64,';

- var expItem_1 = {text: 'Справедливый раздел совместно нажитого имущества', years: '37 дел', dataText: 'Узнайте как мы добьемся справедливого раздела имущества от адвоката Инны'};
- var expItem_2 = {text: 'Взыскание алиментов, изменение размера', years: '22 дела', dataText: 'Узнайте как мы добьемся взыскания алиментов с супруга от адвоката Инны'};
- var expItem_3 = {text: 'Определение места жительства ребенка', years: '42 дела', dataText: 'Узнайте как мы добьемся, чтобы ребенок жил Вами от адвоката Инны'};
- var expItem_4 = {text: 'Установление или оспаривание отцовства', years: '16 дел', dataText: 'Узнайте как мы установим или оспорим отцовство от адвоката Инны'};
- var expItem_5 = {text: 'Лишение и восстановление родительстких прав', years: '12 дел', dataText: 'Узнайте как мы лишим или восстановим родительские права от адвоката Инны'};
- var expItem_6 = {text: 'Определение порядка общения с ребенком', years: '41 дело', dataText: 'Узнайте как мы добьемся справедливого порядка общения с ребенком от адвоката Инны'};

.s-experience
  .container
    .s-experience__header
      +sect-header('За 15 лет практики мы сталкивались<br />со всеми возможными случаями')
    .s-experience__subheader
      +sect-subheader('Выложимся на 100% и добьемся лучшего для Вас решения')
    .s-experience__cols
      .s-experience__col.s-experience__lcol
        +experience-item(expItem_1.text, expItem_1.years, 's-experience', expItem_1.dataText, 'propertyDivision')
        +experience-item(expItem_2.text, expItem_2.years, 's-experience', expItem_2.dataText, 'alimonyCollection')
        +experience-item(expItem_3.text, expItem_3.years, 's-experience', expItem_3.dataText, 'residencePlace')
      .s-experience__col.s-experience__center-col
        .s-experience__decor-cont
          picture.s-experience__picture
            source(srcset=`${imgPlaceholder}`, data-srcset=`${exp_imgPath_webp}` type="image/webp")
            img.s-experience__img(src=`${imgPlaceholder}` data-src=`${exp_imgPath_png}` alt="Деловая практика")
          .s-experience__rounds
            .s-experience__rounds-inner
      .s-experience__col.s-experience__rcol
        +experience-item(expItem_4.text, expItem_4.years, 's-experience', expItem_4.dataText, 'contestingPaternity')
        +experience-item(expItem_5.text, expItem_5.years, 's-experience', expItem_5.dataText, 'parentalRights')
        +experience-item(expItem_6.text, expItem_6.years, 's-experience', expItem_6.dataText, 'communicationOrder')